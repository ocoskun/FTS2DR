#!/bin/csh
set PKG=ca.uol.aig.fts
if (-e ../docs ) then
  echo "Java documents for DR2FTS have existed."
  echo "If you want to re-create it, please delete directory <<docs>>."
else
  mkdir ../docs
  javadoc -sourcepath ../sources ${PKG}.io ${PKG}.message ${PKG}.phasecorrection \
      ${PKG}.fitting ${PKG}.drpipeline ${PKG}.apodization ${PKG}.display -d ../docs
endif
